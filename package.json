{
  "name": "tdd_template",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "5.11.0"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "node ./server.build/bin/www",
    "start:server:dev": "DEV_SERVER=true nodemon ./server/bin/www --exec babel-node",
    "start:client:dev": "webpack-dev-server --inline --content-base client",
    "dev": "concurrently --kill-others 'npm run start:server:dev' 'npm run start:client:dev'",
    "build": "npm run build:server && npm run build:client",
    "build:client": "webpack",
    "build:server": "npm run clean && babel -d ./server.build ./server -s",
    "clean": "rm -rf server.build && mkdir server.build && cp -avr server/* server.build",
    "test": "npm run test:client && npm run test:server",
    "test:client": "mocha --compilers js:babel-core/register --require ./test/test_helper.js  'test/js/**/*.@(js|jsx)'",
    "test:client:watch": "mocha -w --compilers js:babel-core/register --require ./test/test_helper.js  'test/js/**/*.@(js|jsx)'",
    "test:server": "NODE_ENV=test ./node_modules/.bin/mocha test/server --compilers js:babel-core/register --recursive",
    "test:server:watch": "NODE_ENV=test ./node_modules/.bin/mocha --compilers js:babel-core/register test/server -w --recursive"
  },
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.13.2",
    "cookie-parser": "1.3.5",
    "css-loader": "0.25.0",
    "csurf": "1.9.0",
    "debug": "2.2.0",
    "ejs": "2.5.2",
    "express": "4.13.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "immutable": "3.8.1",
    "morgan": "1.6.1",
    "node-sass": "3.10.0",
    "passport": "0.3.2",
    "passport-jwt": "2.2.1",
    "passport-local": "1.0.0",
    "pg": "6.1.0",
    "pg-hstore": "2.3.2",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "redux": "3.6.0",
    "redux-logger": "2.6.1",
    "redux-thunk": "^2.1.0",
    "sass-loader": "4.0.2",
    "sequelize": "3.24.3",
    "sequelize-cli": "2.4.0",
    "serve-favicon": "2.3.0",
    "style-loader": "0.13.1",
    "superagent": "2.3.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.2"
  },
  "devDependencies": {
    "browser-sync": "2.9.6",
    "chai": "3.5.0",
    "concurrently": "3.1.0",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "jsdom": "9.8.0",
    "mocha": "3.1.1",
    "nodemon": "1.11.0",
    "supertest": "2.0.1",
    "webpack-dev-server": "1.15.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  }
}
